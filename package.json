{
  "name": "near-search",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "dev": "vite",
    "deve": "cross-env LOCAL_API=true vite",
    "build": "vue-tsc --noEmit && vite build",
    "buildd": "yarn build && firebase deploy --only hosting",
    "builds": "yarn build && firebase hosting:channel:deploy staging",
    "preview": "vite preview",
    "deploy": "yarn build && firebase deploy",
    "apii": "cd functions & yarn",
    "apie": "cd functions & yarn serve",
    "apit": "cd functions & yarn testdev",
    "apiw": "cd functions & yarn watch",
    "apid": "cd functions & yarn deploy",
    "lint": "eslint --ext .js,.ts --fix .",
    "build_contract": "cd contract && cargo build --target wasm32-unknown-unknown --release",
    "near_deploy": "near deploy srch.near .\\contract\\target\\wasm32-unknown-unknown\\release\\near_search.wasm",
    "contract_optimize_size": "cd contract && wasm-opt -Oz -o target/wasm32-unknown-unknown/release/near_search_optimized.wasm target/wasm32-unknown-unknown/release/near_search.wasm"
  },
  "author": "Danail Irinkov",
  "license": "MIT",

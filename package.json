    "@fortawesome/free-regular-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/vue-fontawesome": "^3.0.0-5",
    "@vueuse/core": "^7.5.5",
    "autolinker": "^3.14.3",
    "axios": "^0.24.0",
    "bn.js": "^5.2.0",
    "buffer": "^6.0.3",
    "deepmerge": "^4.2.2",
    "near-api-js": "^0.44.2",
    "near-contract-parser": "^0.1.3",
    "pinia": "^2.0.0-rc.10",
    "sass": "^1.48.0",
    "vue": "^3.2.27",
    "vue-axios": "^3.4.0",
    "vue-router": "^4.0.12",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@types/bn.js": "^5.1.0",
    "@types/node": "^17.0.15",
